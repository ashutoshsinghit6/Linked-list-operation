class LinkedList:
    class _Node:
        __data__="_element","_next"

        def __init__(self,element,next):
            self._element=element
            self._next=next

    def __init__(self):
        self._head=None
        self._tail=None
        self._size=0

    def __len__(self):
        return self._size
    def is_empty(self):
        return self._size==0
